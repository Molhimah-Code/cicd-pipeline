{
  "name": "cicd-pipeline",
  "version": "1.0.0",
  "description": "CICD Pipeline Application with Multi-Developer Website",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server.js",
    "dev:client": "webpack serve --mode development",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "echo 'Server build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "test:integration": "echo 'Integration tests not implemented yet'",
    "test:e2e": "echo 'E2E tests not implemented yet'",
    "test:staging": "echo 'Staging tests not implemented yet'",
    "test:smoke": "echo 'Smoke tests not implemented yet'",
    "performance:check": "echo 'Performance checks not implemented yet'",
    "security:scan": "npm audit --audit-level high"
  },
  "keywords": [
    "cicd",
    "pipeline",
    "ci-cd",
    "nodejs",
    "express"
  ],
  "author": "CICD Pipeline Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cicd-pipeline.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/cicd-pipeline/issues"
  },
  "homepage": "https://github.com/your-username/cicd-pipeline#readme"
}
